}

.header-inner {
  width: 1060px;
  margin: 0 auto;
  text-align: left;
}
.header-inner::after {
  display: block;
  clear: both;
  content: '';
}
.header-inner .site-logo {
  margin: 0;
}

.header-inner-left {
  /*width: 740px;*/
  width: 830px;
  float: left;
}

.header-inner-right {
  /*width: 320px;*/
  width: 230px;
  float: right;
  text-align: right;
}

.header-olympic-schedule {
  display: -ms-flex-box;
  display: flex;
  margin: 5px 0;
  letter-spacing: -.5em;
  /*font-size: .875em;*/
  font-size: .8em;
}
.header-olympic-schedule dt,
.header-olympic-schedule dd {
  display: inline-block\9;
  padding: 3px 10px;
  border: 2px solid #002063;
  letter-spacing: normal\9;
}
.header-olympic-schedule dt {
  background: #002063;
  color: white;
}
.header-olympic-schedule dd {
  margin: 0;
  background: white;
  color: #002063;
}
.header-olympic-schedule dd:not(:last-of-type) {
  margin-right: 10px;
}

.olympic-logo {
  max-width: 260px;
  max-height: 99px;
}

.reseller-text {
  clear: both;
  margin: 0;
  text-align: right;
  color: #fff799;
  font-family: 'Arial';
  font-size: .75em;
  text-shadow: 0px 1px 1px #000000;
}

/*---------------------------------------------
  Global Navigaion
  ---------------------------------------------*/
.navigation {
  padding: 1em 1em;
  background: #002063;
}

.navigation-inner {
  width: 1060px;
  margin: 0 auto;
}
.navigation-inner::after {
  display: block;
  clear: both;
  content: '';
}

.globalnav {
  width: 510px;
  float: left;
  text-align: left;
}
.globalnav > ul {
  list-style: none;
  margin: 0;
  padding: 0;
  font-weight: bold;
  font-size: 1.125em;
}
.globalnav > ul > li {
  position: relative;
  display: inline-block;
  padding-right: 10px;
}
.globalnav > ul > li:not(:first-child) {
  margin-left: 5px;
}
.globalnav > ul > li:not(:last-child)::before {
  position: absolute;
  top: 3px;
  right: 0;
  display: block;
  width: 1px;
  height: 20px;
  background: #6ab3e7;
  content: '';
}
.globalnav > ul > li a {
  text-decoration: none;
  color: white;
  transition: all .25s ease;
}
.globalnav > ul > li a:hover {
  color: #fff45c;
}
.globalnav > ul > li:not(:first-child) a::before {
  margin-right: .25em;
  font-family: 'FontAwesome';
  font-weight: normal;
  content: '\f0a9';
}
.globalnav .home a::before {
  font-family: 'FontAwesome';
  font-weight: normal;
  content: '\f015';
}

.header-subnav {
  width: 550px;
  float: right;
  list-style: none;
  margin: 3px 0;
  padding: 0;
  text-align: right;
  font-size: .875em;
}
.header-subnav > li {
  display: inline-block;
}
.header-subnav > li:not(:last-child) {
  position: relative;
  margin-right: 3px;
  padding-right: 7px;
}
.header-subnav > li:not(:last-child)::before {
  position: absolute;
  top: 3px;
  right: 0;
  display: block;
  width: 1px;
  height: 15px;
  background: white;
  content: '';
}
.header-subnav > li a {
  text-decoration: none;
  color: white;
  transition: all .25s ease;
}
.header-subnav > li a:hover {
  color: #fff45c;
}

